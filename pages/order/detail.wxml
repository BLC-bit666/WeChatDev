			<u-steps-item vue-id="{{('a2b0478a-3')+','+('a2b0478a-1')}}" title="厨师出发" bind:__l="__l" vue-slots="{{['icon']}}">
				<image class="step-image" slot="icon" src="https://cdn.jsdelivr.net/gh/Lanailearn/img/img/2_cur.png"></image>
			</u-steps-item>
			<u-steps-item vue-id="{{('a2b0478a-4')+','+('a2b0478a-1')}}" title="厨师到达" bind:__l="__l" vue-slots="{{['icon']}}">
