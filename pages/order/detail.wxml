	<view class="w94 step-view mt20">
		<view class="box-view bold">
			<view class="dot"></view><label>订单信息</label>
		</view>
		<view class="box-view center h50" style="margin-top:10rpx;"><label style="color:#999;">订单编号</label>
			<view class="r-label" style="display:flex;">20231025025727756084372<u-icon vue-id="a2b0478a-10" name="file-text" size="18" color="#999" bind:__l="__l"></u-icon>
