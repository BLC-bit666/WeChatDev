			<u-steps-item vue-id="{{('a2b0478a-6')+','+('a2b0478a-1')}}" title="服务完成" bind:__l="__l" vue-slots="{{['icon']}}">
				<image class="step-image" slot="icon" src="https://cdn.jsdelivr.net/gh/Lanailearn/img/img/5.png"></image>
			</u-steps-item>
		</u-steps>
	</view>
