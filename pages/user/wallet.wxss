
page {
	background: #eee;
}
.template-view {
	width: 100%;
	height: 250rpx;
	border-radius: 20rpx;
	position: relative;
	margin-top: 20rpx;
	box-shadow: rgb(36, 195, 115) 0px 5px 10px;
	/*background: url('https://cdn.jsdelivr.net/gh/Lanailearn/img/img/walletback.png');
	background-size: 100%;
	background-repeat: no-repeat; */
}
.walletBack {
	width: 100%;
	height: 250rpx;
	position: absolute;
	top: 0rpx;
}
.balance-title {
	width: 90%;
	margin: 0 auto;
	margin-top: 30rpx;
	color: #fff;
	font-size: 30rpx;
	position: relative;
	z-index: 99;
	padding-top: 30rpx;
}
.balance-value {
	width: 90%;
	margin: 0 auto;
	margin-top: 20rpx;
	color: #fff;
	font-size: 60rpx;
	font-weight: bold;
	position: relative;
	z-index: 99;
}
.passageway {
	width: 100%;
	/* background: #fff; */
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 40rpx;
}
.pass-item {
	width: 25%;
	height: 60rpx;
	line-height: 60rpx;
	border-radius: 20rpx;
	border: 2rpx solid #24c373;
	text-align: center;
	color: #24c373;
}
.money-view {
	width: 100%;
	background: #fff;
	border-radius: 20rpx;
	display: flex;
	justify-content: space-around;
	flex-wrap: wrap;
	padding: 10rpx 0rpx;
	margin-top: 30rpx;
}
.money-item {
	width: 48%;
	height: 150rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: contain;
	display: flex;
	justify-content: space-around;
	margin-top: 10rpx;
}
.left-view {
	width: 50%;
	height: 130rpx;
	display: flex;
	align-items: center;
	justify-content: space-around;
	font-weight: bold;
	color: #24c373;
	font-size: 48rpx;
}
.right-view {
	width: 50%;
	height: 125rpx;
	display: flex;
	align-items: flex-end;
	justify-content: space-around;
	color: #24c373;
}
.bottom-view {
	width: 96%;
	/* margin:0 auto; */
	left: 2%;
	background: #24c373;
	color: #fff;
	height: 80rpx;
	line-height: 80rpx;
	text-align: center;
	font-size: 30rpx;
	border-radius: 20rpx;
	position: fixed;
	bottom: 20rpx;
}

